<Application x:Class="ControlsDefualtStyle.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ControlsDefualtStyle"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>

            <ResourceDictionary.MergedDictionaries>



                <ResourceDictionary Source="Themes/WindowStyle.xaml"/>

                <ResourceDictionary Source="Themes/ButtonStyle.xaml"/>

                <ResourceDictionary Source="Themes/TextBoxStyle.xaml"/>

                <ResourceDictionary Source="Themes/PasswordBoxStyle.xaml"/>

                <ResourceDictionary Source="Themes/CheckBoxStyle.xaml"/>

                <ResourceDictionary Source="Themes/TextBlockStyle.xaml"/>

                <ResourceDictionary Source="Themes/ExpanderStyle.xaml"/>

                <ResourceDictionary Source="Themes/ScrollBarStyle.xaml"/>

                <ResourceDictionary Source="Themes/TreeViewStyle.xaml"/>

                <ResourceDictionary Source="Themes/TooltipStyle.xaml"/>


            </ResourceDictionary.MergedDictionaries>

        </ResourceDictionary>
    </Application.Resources>
</Application>
